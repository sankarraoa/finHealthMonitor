-- First, verify what will be deleted
SELECT COUNT(*) as records_to_delete
FROM parties p
WHERE p.party_type = 'person' 
  AND p.organization_id IS NULL;

-- If the count looks correct, proceed with deletion
DELETE FROM parties
WHERE party_type = 'person' 
  AND organization_id IS NULL;

-- Verify deletion
SELECT COUNT(*) as remaining_null_org_persons
FROM parties p
WHERE p.party_type = 'person' 
  AND p.organization_id IS NULL;
